---
import PrimaryButton from './buttons/PrimaryButton.astro'
import { links } from '../utils/links'
---
<nav class="nav-wrapper">
    <img src="/images/text-logo.svg" alt="keyhunt logo">
    <PrimaryButton text='GET STARTED' href={links.home}/>
</nav>

<style lang="scss">
    .nav-wrapper{
        width: 100%;
        height: 70px;
        padding: var(--space-l);
        display: flex;
        align-items: center;
        justify-content: space-between;

        img{
            max-height: 100%;
            height: 60%;
            width: auto;
        }
    }
</style>